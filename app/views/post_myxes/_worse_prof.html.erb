<div class="col195 clubBar">
  <div class="col195in">
    <h1 class="col195Title">Worse Teacher/Prof</h1>
    <div class="col195Cont">
      <%
      worseProf = PostMyx.prof_filter("Worse")
    %>
      <% if worseProf.size > 0 %>
        <%
        worseProf.each do |wp|
        %>
          <div class="teacherItem">
            <div class="recentImg"><a href="#"><div><img src="pictures/pic2.png"/></div></a></div>
            <div class="teacherInf">
              <div class="recentName"><a href="#"><%= wp.professor   %></a></div>
              <div class="recentDes"><%= wp.post.school.name %></div>
            </div>
          </div>
        <%
        end
      %>
        <div class="dueDbutton"><%= link_to "View more Worse Teacher/Prof", more_worse_post_myxes_path %></div>
      <%
      end
    %>

    </div>

  </div>
</div>





