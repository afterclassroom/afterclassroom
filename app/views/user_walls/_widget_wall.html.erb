    	<div id="wall_page_wrap">
        <% @walls.each do |w| %>
        <%= render :partial => "wall_item", :locals => {:wall => w} %>
        <% end %>
		</div>
		<%= render :partial => "alert" %>
        <div class="goTop">
            <a href="javascript:;" onclick="goToByScroll('top_page')">Top</a>
        </div>
    </div>
</div>
<%= render :partial => 'shared/javascript_load' %>
<script type="text/javascript">
    $(document).ready(function(){
				//on page load call tb_init  
        tb_init('div.wall_page_<%= @page %> a.thickbox');//pass where to apply thickbox
        imgLoader = new Image();// preload image
        imgLoader.src = tb_pathToImage;
    });
</script>