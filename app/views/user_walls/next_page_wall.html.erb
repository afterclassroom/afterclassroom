<% @walls.each do |w| %>
	<% if w.wall_parent %>
              <%= render :partial => "user_walls/wall_item_with_parent", :locals => {:wall => w} %>
            <% else %>
              <%= render :partial => "user_walls/wall_item", :locals => {:wall => w} %>
            <% end %>
<% end %>
<%= render :partial => 'shared/javascript_load' %>
<script type="text/javascript">
    $(document).ready(function(){
				//on page load call tb_init  
        tb_init('div.wall_page_<%= @page %> a.thickbox');//pass where to apply thickbox
        imgLoader = new Image();// preload image
        imgLoader.src = tb_pathToImage;
    });
</script>
<script>
$(function(){
    $('.AfterDes').imagefit();
});
$(function(){
    $('img').load(function(){ $('.AfterClCom').imagefit() });
});
</script>
