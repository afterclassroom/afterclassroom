$("#show_jplayer_<%= @user_wall_video.user_wall.id %>").html('<div id="player_<%= @user_wall_video.user_wall.id %>"></div>');
  var so_<%= @user_wall_video.user_wall.id %> = new SWFObject('/javascripts/player.swf','player_<%= @user_wall_video.user_wall.id %>','320','240','9','#000000');
  so_<%= @user_wall_video.user_wall.id %>.addParam('allowfullscreen','true');
  so_<%= @user_wall_video.user_wall.id %>.addParam('allowscriptaccess','always');
  so_<%= @user_wall_video.user_wall.id %>.addParam('wmode','opaque');
  so_<%= @user_wall_video.user_wall.id %>.addVariable('autostart','true');
  so_<%= @user_wall_video.user_wall.id %>.addVariable('file','<%= @user_wall_video.link %>');
  so_<%= @user_wall_video.user_wall.id %>.write('player_<%= @user_wall_video.user_wall.id %>');