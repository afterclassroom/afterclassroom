<% content_for :menu_top_student_lounge do -%>
<%= render :partial => '/shared/submenu' %>
<% end -%>
<div class="slStoryL">
    <div class="contDetL">
        <div class="contDetLin">
            <div class="inboxLeft">
                <div class="slsShare">
                    <div class="slsShareL">
                        <span>You are here:</span>
                        <%= link_to "Photos", user_photos_url(current_user), :class => "slsShareLink" %>
                        <a href="javascript:;" class="slsShareLinkL"><%= current_user == @photo_album.user ? "Your photos" : "Friends shared photos" %></a>
                    </div>
                    <div class="slsShareR">
                        <%= render :partial => "menu" %>
                    </div>
                </div>
                <%= display_flash_messages_with_raw %>
                <%= render :partial => "expand_create_photo_album" %>
                <div class="stoDet phoD">
                    <div class="AsDcomImg">
                        <%= show_image_user_post(@photo_album.user) %>
                    </div>
                    <div class="stoDetR">
                        <div class="stoDetRT">
                            <%= @photo_album.user.full_name %>: <%= @photo_album.name %>
                        </div>
                    </div>
                </div>
                <div class="ad-preloads">
                </div>
                <div class="AsDOr RePho">
                    <%= render :partial => "related_photo" %>
                </div>
                <%= render :partial => "photo_album_action" %>
                <%= show_post_comments(@photo_album) %>
                <div class="goback" id="go_back">
                    <%= show_go_back %>
                </div>
                <div class="AsDOr">
                    <h1 class="titleH1">Another photo albums</h1>
                    <% @another_photo_albums.each do |pb| %>
                    <div class="AsDOrItem">
                        <%= link_to pb.name, show_album_user_photos_url(current_user, :photo_album_id => pb) %>
                    </div>
                    <% end %>
                </div>
                <div class="goTop">
                    <a href="javascript:;" onclick="goToByScroll('top_page')">Top</a>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="contMainR">
    <!--Most Viewed-->
    <%= render :partial => "photos/most_viewed" %>
    <!--Most Viewed-->
    <!--Ads-->
    <%= render :partial => "ads/show_vertical_ads" %>
    <!--Ads-->
</div>
