<div class="photoItem">
  <div class="photoImg"><%= link_to "<div>#{image_tag photo.photo_attach.url(:thumb)}</div>", user_photo_path(current_user, photo) %></div>
  <div class="photoName"><%= photo.photo_album.name %></div>
  <div class="photoInf"><%= pluralize(photo.photo_album.photos.size, "photo") %></div>
</div>
