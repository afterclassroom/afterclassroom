<div class="photoItem">
  <div class="photoImg"><%= link_to raw("<div>#{image_tag photo.photo_attach.url(:thumb)}</div>"), user_photo_path(current_user, photo) %></div>
  <div class="photoInf" style="margin-top: -5px;"><%= pluralize(photo.photo_album.photos.size, "photo") %></div>
  <div class="photoNamePhoto"><span><%= photo.photo_album.name %></span></div>
</div>

