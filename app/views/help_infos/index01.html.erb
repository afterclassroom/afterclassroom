<h1>Listing help_infos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @help_infos.each do |help_info| %>
    <tr>
      <td><%= help_info.title %></td>
      <td><%= help_info.content %></td>
      <td><%= link_to 'Show', help_info %></td>
      <td><%= link_to 'Edit', edit_help_info_path(help_info) %></td>
      <td><%= link_to 'Destroy', help_info, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Help info', new_help_info_path %>