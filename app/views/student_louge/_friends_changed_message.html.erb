<ul>Friends changed their message
	<% for friend_change in friends_change %>
	<li>
		<% if current_user == friend_change.user %>
			<% if friend_change.user == friend_change.flirting_chanel.user %>
				<%= image_tag friend_change.flirting_chanel.user_target.avatar.url(:thumb) %>
			<% else %>
				<%= image_tag friend_change.flirting_chanel.user.avatar.url(:thumb) %>
			<% end %>
		<% else %>
			<%= image_tag friend_change.user.avatar.url(:thumb) %>
		<% end %>
	</li>
	<% end %>
