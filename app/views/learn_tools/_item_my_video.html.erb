<div id="each_vid_<%= video.id %>" class="musicL">
  <div class="photoImg"><%= display_image_video(video) %><%#= link_to display_image_video(video), user_video_url(current_user, video) %></div>
  <div class="mostMSR">
    <div class="mostMSA"><%#= link_to "", user_video_url(current_user, video), :class => "mostMSPlay" %></div>
  </div>
  <div class="musicB">
    <div class="musicN"><%= video.title %><%#= link_to video.title, user_video_url(current_user, video) %></div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function(){
    var eachItem = $('div#each_vid_<%= video.id %>');
    
    eachItem.click(function(){
      $(this).find('div.photoImg div').css('background',"url('/images/bgphoto_sel.png') no-repeat scroll left top transparent");
      
      if (overalObject == null){
        overalObject = eachItem;
      }else {
        overalObject.find('div.photoImg div').css('background',"url('/images/bgphoto.png') no-repeat scroll left top transparent");
        overalObject = eachItem;
      }
      var filename = overalObject.find('div.musicN').text();
      jQuery.data(overalObject,"vid_name",filename);
      
      overVidImg = $(this).find('div div');
      
    });

    eachItem.mouseover(function() {
      $(this).css('cursor', 'pointer');
    });
  });
</script>