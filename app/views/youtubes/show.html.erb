<h1><%= @video.title %></h1>
	by <%= link_to @video.author.name, "http://youtube.com/#{@video.author.name}" %>
  <%= youtube_embed @video %>

  <%= link_to 'Back', user_youtubes_path(current_user) %>
  <%= link_to 'Edit/Update', edit_user_youtube_path(current_user, @video) %>
  <%= link_to 'Delete', user_youtube_path(current_user, @video), :method => "delete" %>