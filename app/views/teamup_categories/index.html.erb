<% content_for :header do -%>
	Teamup categories
<% end -%>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for teamup_category in @teamup_categories %>
  <tr>
    <td><%=h teamup_category.name %></td>
    <td><%= link_to 'Show', teamup_category %></td>
    <td><%= link_to 'Edit', edit_teamup_category_path(teamup_category) %></td>
    <td><%= link_to 'Destroy', teamup_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New teamup_category', new_teamup_category_path %>
