<% end -%>
<div class="slStoryL">
    <div class="contDetL">
        <div class="contDetLin">
            <div class="inboxLeft">
                <div class="slsShare">
                    <div class="slsShareL">
                        Music Albums
                    </div>
                    <div class="slsShareR">
                        <%= render :partial => "musics/menu" %>
                    </div>
                </div>
                <%= display_flash_messages_with_raw %>
                <%= render :partial => "musics/expand_create_music_album" %>
                <div id="my_musics_list">
                    <div class="slsTop">
                        <div class="musicRow" id="mytable">
                            <% @my_music_albums.each do |m_p| %>
                            <%= render :partial => "item_my_album", :locals => {:album => m_p} %>
                            <% end %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="contMainR">
    <!--Most Viewed-->
    <%= render :partial => "musics/most_viewed" %>
    <!--Most Viewed-->
    <!--Ads-->
    <%= render :partial => "ads/show_vertical_ads" %>
    <!--Ads-->
</div>

<%= render :partial => "action_box_menu" %>

<script type="text/javascript">
    function submit(){
        $("#form_new").submit();
    }
    
    $().ready(function(){
        $("#form_new").validate({
            errorLabelContainer: $("#form_new div.stoError")
        });
    });
</script>
