<div class="col195 student195">
  <div class="col195in">
    <h1 class="col195Title">Good organizations</h1>
    <div class="col195Cont">
      <%
      good_org = PostTeamup.with_limit.good_org(@school)
    %>
      <% if good_org.size > 0 %>
        <%
        good_org.each do |tt|
        %>
          <div class="studentItem">
            <div class="studentImg"><%= show_image_user_post(tt.post.user) %></div>
            <div class="studentInf"><%= link_to truncate_words(tt.post.title, 4), post_teamup_path(tt) %></div>
          </div>
        <% end %>
        <div class="dueDbutton"><%= link_to "View more Bad organizations", good_org_post_teamups_path %></div>
      <% end %>
    </div>
  </div>
</div>

