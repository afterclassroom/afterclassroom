<div class="col195 student195">
  <div class="col195in">
    <h1 class="col195Title">TeamUp for Start-up</h1>
    <div class="col195Cont">
      <%
      teamStartup = PostTeamup.with_school(@school).recent.with_limit
    %>
      <% if teamStartup.size > 0 %>
        <%
        teamStartup.each do |ts|
        %>
          <div class="studentItem">
            <div class="studentImg"><%= link_to raw("<div>#{image_tag(ts.post.user.avatar.url(:thumb))}</div>"), {:controller => "profile", :action => "show_profile", :id => ts.post.user.id} %></div>
            <div class="studentInf"><%= link_to truncate_words(ts.post.title, 4), post_teamup_path(ts) %></div>
          </div>
        <%
        end
      end
    %>
    </div>
  </div>
</div>
