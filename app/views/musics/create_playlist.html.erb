<playlist version='1' xmlns='http://xspf.org/ns/0/'>
  <title><%= @music_album.name %></title>
  <info><%= pluralize(@music_album.musics.size, "song") %></info>
  <trackList>
    <% @music_album.musics.each do |music| %>
      <track>
        <title><%= music.title %></title>
        <location><%= music.music_attach.url %></location>
      </track>
    <% end %>
  </trackList>
</playlist>