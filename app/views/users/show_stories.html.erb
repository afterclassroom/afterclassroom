<% content_for :menu_top_student_lounge do -%>
<%= render :partial => 'users/show_public/submenu' %>
<% end -%>
<div class="contMainC">
    <%= render :partial => "users/show_public/proc" %>
    <%= render :partial => "users/show_public/infor" %>
    <%= render :partial => "users/show_public/friends" %>
    <%#= render :partial => "/users/show_public/links" %>
</div>
<div class="contMainL">
    <div class="col571 proCen">
        <div class="col571in">
            <div class="proCenCont">
                <% @stories.each do |s| %>
                <%= render :partial => "users/show_public/item_story", :locals => {:story => s} %>
                <% end %>
                <!--Pagination-->
                <div class="assPage">
                    <span class="numPage"><%= page_entries_info @stories %></span>
                    <%= will_paginate @stories %>
                </div>
                <!--Pagination-->
            </div>
        </div>
    </div>
</div>
<div class="contMainR">
    <!--Ads-->
    <%= render :partial => "ads/show_vertical_ads" %>
    <!--Ads-->
</div>
<script type="text/javascript" charset="utf-8">
$(document).ready(function(){
    $('.iframe').fancybox({
      'width': 1100,
      'height': 600,
      'autoScale': false,
      'hideOnContentClick': false,
      'type': 'iframe'
    });
  });
</script>
