<% content_for :menu_top_student_lounge do -%>
  <%= render :partial => 'submenu' %>
<% end -%>
<div class="contMainC">
  <%= render :partial => "proc" %>
  <%= render :partial => "infor" %>
  <%= render :partial => "friends" %>
<%#= render :partial => "links" %>
</div>
<div class="contMainL">
  <div class="col571 proCen" id="user_wall">
    <%= render :partial => "user_walls/wall_form_ajax" %>
    <div class="col571in">
      <div class="proCenCont">
        <div id="wall_page_wrap">
          <% @walls.each do |w| %>
            <% if w.wall_parent %>
                    	<%= render :partial => "user_walls/wall_item_with_parent", :locals => {:wall => w} %>
										<% else %>
											<%= render :partial => "user_walls/wall_item", :locals => {:wall => w} %>
                    <% end %>
          <% end %>
					<div id="wall_page_wrap_footer"></div>
        </div>
        <%= render :partial => "user_walls/alert" %>
        <div class="goTop">
          <a href="javascript:;" onclick="goToByScroll('top_page')">Top</a>
        </div>
      </div>
    </div>
    <%= render :partial => "user_walls/next_page_wall_js", :locals => {:user_id => current_user.id} %>
  </div>
</div>
<div class="contMainR">
  <!--Ads-->
  <%= render :partial => "ads/show_vertical_ads" %>
  <!--Ads-->
</div>
<script>
$(window).load(function(){
    $('img').load(function(){
      $('.AfterClCom').imagefit();
      $('.AfterDes').imagefit();
    });
});
</script>
