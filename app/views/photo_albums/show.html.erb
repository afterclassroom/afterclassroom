<% content_for :menu_top_student_lounge do -%>
<%= render :partial => '/shared/submenu' %>
<% end -%>
<div class="slStoryL">
    <div class="contDetL">
        <div class="contDetLin">
            <div class="inboxLeft">
                <div class="slsShare">
                    <div class="slsShareL">
                        Photo Albums
                    </div>
                    <div class="slsShareR">
                        <%= render :partial => "menu_show" %>
                    </div>
                </div>
                <%= display_flash_messages_with_raw %>
                <div id="create_a_album" style="display:none;">
                    <%= render :partial => "photos/widget_form" %>
                </div>
                <div id="my_photos_list">
                    <div class="slsTop">
                        <div class="slsTopLT">
                            <%= @photo_album.name %>
                        </div>
                        <div class="photoRow" id="mytable">
                            <% @photos.each do |m_p| %>
                            <%= render :partial => "item_my_photo", :locals => {:photo => m_p} %>
                            <% end %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="contMainR">
    <!--Most Viewed-->
    <%= render :partial => "photos/most_viewed" %>
    <!--Most Viewed-->
    <!--Ads-->
    <%= render :partial => "ads/show_vertical_ads" %>
    <!--Ads-->
</div>
