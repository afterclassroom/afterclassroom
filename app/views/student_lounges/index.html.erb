      </div>
    </div>
  </div>
  <!--Ads-->
  <%= render :partial => "ads/show_vertical_ads" %>
  <!--Ads-->
</div>

<script>
<% if @user_invite_chat %>
    window.onload(invite_chat('<%= @user_invite_chat.id %>'));
<% end %>
  friends_changed_message();
