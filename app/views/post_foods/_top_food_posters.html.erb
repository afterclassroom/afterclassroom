<div class="col195 Poster">
  <div class="col195in">
    <h1 class="col195Title">Top food posters</h1>
    <div class="col195Cont">
      <div class="posterItem">
        <% users = PostFood.top_food_posters %>
        <% users.each do |user| %>
          <div class="posterL"><%= link_to "<div>#{image_tag(user.avatar.url(:thumb))}</div>", {:controller => "profile", :action => "show_profile", :id => user.id} %></div>
        <% end %>
      </div>
    </div>
  </div>
</div>
